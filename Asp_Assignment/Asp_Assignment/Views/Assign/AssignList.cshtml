@model List<Asp_Assignment.Context.tbl_assign>
@{
    ViewBag.Title = "AssignList";
}

<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>FamilyName</th>
        <th>Address</th>
        <th>ZipCode</th>
        <th>City</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Action</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.FamilyName</td>
            <td>@item.Address</td>
            <td>@item.ZipCode</td>
            <td>@item.City</td>
            <td>@item.Phone</td>
            <td>@item.Email</td>
            <td>
                <a href="@Url.Action("Assign", new {item.Name, item.Email, item.Address, item.Phone, item.FamilyName, item.ZipCode, item.City})" class="btn btn-success">Edit</a>
                <a href="@Url.Action("Delete", new {item.Name})" class="btn btn-danger">Delete</a>
            </td>

        </tr>

     }
</table>
